#include <string>
#include <stdexcept>
using namespace std;

namespace Point{
class Point
{
private:
    int x;
    int y;
public:
    Point(){
        x=0;
        y=0;
    };
    Point(int x_pos,int y_pos){
        x=x_pos;
        y=y_pos;
    };
    string toString();
    void setPosition(int x_pos,int y_pos);
    int getX_pos();
    int getY_pos();
};
}